{
  "name": "video-enhance",
  "productName": "ArSrNaESRGAN-Video",
  "version": "0.0.1",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "asar": "asar pack ./outputs/ArSrNaESRGANVideo-win32-x64/resources/app app.asar",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "electron-packager ./ ArSrNaESRGANVideo --win --ignore test/ --ignore electron-v19.0.4-win32-x64.zip --arch=x64 --out ./out --electron-zip-dir=./ --icon=./icon.ico --overwrite"
  },
  "keywords": [],
  "author": {
    "name": "Ar-Sr-Na",
    "email": "root@arsrna.cn"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "extraResources":"./src/backres/"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "video_enhance"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "express-ws": "^5.0.2",
    "node-mediainfo": "^1.0.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "electron": "19.0.4",
    "electron-reload": "^2.0.0-alpha.1"
  }
}
