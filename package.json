{
  "name": "video-enhance",
  "productName": "ArSrNaESRGAN-Video",
  "version": "2.1.0",
  "description": "Powered by Ar-Sr-Na",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "asar": "asar pack ./outputs/ArSrNaESRGANVideo-win32-x64/resources/app app.asar",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "pack": "electron-builder --dir",
    "build": "electron-packager ./ ArSrNaESRGANVideo --win --ignore test --ignore resources --ignore electron-v19.0.4-win32-x64.zip --arch=x64 --out ./out --electron-zip-dir=./ --icon=./icon.ico --overwrite"
  },
  "keywords": [],
  "author": {
    "name": "Ar-Sr-Na",
    "email": "root@arsrna.cn"
  },
  "build": {
    "productName": "ArSrNaESRGANVideo",
    "appId": "arsrnaesrganvid",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "./icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": "ia32"
        }
      ],
      "asarUnpack": [
        "text/*",
        "electron-v19.0.4-win32-x64.zip"
      ],
      "nsis": {
        "oneClick": false,
        "allowElevation": true,
        "allowToChangeInstallationDirectory": true,
        "installerIcon": "./icon.ico",
        "uninstallerIcon": "./icon.ico",
        "installerHeaderIcon": "./icon.ico",
        "createDesktopShortcut": true,
        "createStartMenuShortcut": true,
        "shortcutName": "ArSrNaESRGAN视频版"
      },
      "electronDownload": {
        "mirror": "https://npmmirror.com/mirrors/electron/"
      }
    }
  },
  "license": "GPL 3.0",
  "config": {
    "forge": {
      "packagerConfig": {
        "extraResources": "./src/backres/"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "video_enhance"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "cos-nodejs-sdk-v5": "^2.11.12",
    "crypto": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "express-ws": "^5.0.2",
    "node-mediainfo": "^1.0.3",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "electron": "19.0.4",
    "electron-packager": "^15.5.1",
    "electron-reload": "^2.0.0-alpha.1"
  }
}
